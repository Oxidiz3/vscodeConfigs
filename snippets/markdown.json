	// 	],
	// 	"description": "Log output to console"
	// }
	"Python": {
		"prefix": "python",
		"body": [
			"```python",
			"$1",
			"```"
		],
		"description": "Add a code snippet for python"
	},
	"SQL": {
		"prefix": "sql",
		"body": [
			"```sql",
			"$1",
			"```"
		],
		"description": "Add a code snippet for sql"
	}
}